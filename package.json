{
  "name": "teslaterm",
  "version": "0.1.0",
  "scripts": {
    "build": "cross-var tsc",
    "watch": "cross-var tsc -w",
    "lint": "cross-var tslint -c tslint.json -p tsconfig.json",
    "start-electron": "npm run build && cross-var electron . --no-sandbox",
    "start-node": "npm run build && webpack && node js/main/main_node.js",
    "install-electron": "cd node_modules/@serialport/bindings && prebuild-install --tag-prefix @serialport/bindings@ -r electron -t 7.2.1",
    "install-node": "cd node_modules/@serialport/bindings && prebuild-install --tag-prefix @serialport/bindings@ -r node",
    "package-electron": "cross-var electron-packager . --overwrite --platform=linux --arch=x64 --out=dist --prune=true "
  },
  "main": "js/main/main_electron.js",
  "dependencies": {
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "ini": "^1.3.5",
    "jquery": "^3.4.1",
    "jspack": "0.0.4",
    "justgage": "^1.3.5",
    "jzz": "^0.6.9",
    "midi-player-js": "^2.0.5",
    "node": "^14.2.0",
    "serialport": "^9.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@types/chrome": "0.0.78",
    "@types/ini": "^1.3.30",
    "@types/jquery": "^3.3.34",
    "@types/microtime": "^2.1.0",
    "@types/node": "^10.17.18",
    "@types/serialport": "^8.0.0",
    "@types/w2ui": "^1.4.32",
    "@types/webmidi": "^2.0.4",
    "cross-var": "^1.1.0",
    "electron": "^7.2.1",
    "electron-packager": "^14.2.1",
    "npm": "^6.14.4",
    "prebuild-install": "^5.3.3",
    "ts-loader": "^7.0.3",
    "tslint": "^5.10.0",
    "tslint-no-circular-imports": "^0.7.0",
    "typescript": "^3.8.3"
  }
}
